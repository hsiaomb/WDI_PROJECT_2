<!DOCTYPE html>
<html>
<head>
  <title>GiftWish</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<ul>
<li><a href="/users/sign_in">Users</a></li>
<li><a href="/products/new">products</a></li>
<li><a href="/categories/new">categories</a></li>
</ul>
<% if user_signed_in? %>
Hola, <%=  current_user.username %>! | <%= link_to "Edit Account Details", edit_user_registration_path%>
<%= link_to "Logout", destroy_user_session_path, method: :delete %>
<% else %>
<%= link_to "Login", new_user_session_path %> | <%= link_to "Signup", new_user_registration_path%>
<% end %>
<%= yield %>

</body>
</html>
