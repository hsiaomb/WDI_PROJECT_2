    <div id="fullpage">
      <div class="section">
        <div class="row">
          <div class="panel panel-default col-md-4 col-md-offset-6">
            <div class="panel-heading">
              <h3 class="panel-title">Log In</h3>
            </div>
            <div class="panel-body">
              <form class="form-inline">
                <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                <div class="form-group">
                  <%= f.label :email, :class=>'sr-only'%><br />
                  <%= f.text_field :email, :class=>'form-control', :placeholder=>"Email" %>
                </div>

                <div class="form-group">
                  <%= f.label :password, :class=>'sr-only' %><br />
                  <%= f.password_field :password, autocomplete: "off", :class=>'form-control', :placeholder=>"Password" %>
                </div>
                <% if devise_mapping.rememberable? -%>
                <div class="field">
                  <%= f.check_box :remember_me %>
                  <%= f.label :remember_me %>
                </div>
                <% end -%>  
              </form>
            </div>
            <div class="panel-footer">
              <div class="actions">
              <%= f.submit "Log in", :class=>"btn btn-default" %>
                <%= link_to "Signup", new_user_registration_path%>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="section" id="home_two">
        <div class="slide"> <p class = "col-md-4 col-md-offset-6">How It Works<p></div>
        <div class="slide"> 
          <p class = "col-md-4 col-md-offset-6">Make an Account.</p>
        </div>
        <div class="slide"> 
          <p class = "col-md-4 col-md-offset-6">Add a new product into your wishlist. <br />
            Or see what exciting new products other people have added to their wishlists.</p>
          </div>
          <div class="slide"> <p class = "col-md-4 col-md-offset-6">Relieve your friends & family from the stress of finding you that perfect gift.<p></div>
        </div>
      </div>
      <% end %>



